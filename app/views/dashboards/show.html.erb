<div class="container">
  <% @user_bookings.each do |booking| %>
    <% booking.instrument.photos.first %>
    <h2><%= booking.instrument.style %></h2>
    <h4><%= booking.instrument.rate %></h4>
    <h4><%= booking.instrument.location %></h4>
    <p><%= booking.start_time %></p>
    <p><%= booking.end_time %></p>
    <p><%= booking.total_price %></p>
  <% end %>
</div>


<%= link_to "Back to bookings", instruments_path, class: "btn btn-primary" %>